<If "%{REMOTE_ADDR} == '127.0.0.1'">

 <IfModule mod_headers.c>
   <FilesMatch ".+\.(txt)$">
     Header unset Cache-Control
     Header set Cache-Control "max-age=300, public"
   </FilesMatch>
 </IfModule>

 <IfModule mod_expires.c>
   ExpiresActive On
   ExpiresDefault "access plus 5 minutes"
 </IfModule>

</If>
